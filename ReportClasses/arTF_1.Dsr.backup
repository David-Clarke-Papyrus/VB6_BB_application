Attribute VB_Name = "arTF_1"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Dim LineArray() As String
Dim LineTotalArray() As String
Dim iCurRow As Integer

Public Sub Component(pLineArray As Variant, pTotalValues As String, pTotalDescription As String)
    On Error GoTo errHandler
    LineArray = pLineArray
    iCurRow = 1
    Me.tTotalValues = pTotalValues
    Me.fTotalDescription = pTotalDescription
    Exit Sub
errHandler:
    If ErrMustStop Then Debug.Assert False: Resume
    ErrorIn "arTF_1.Component(pLineArray,pTotalValues,pTotalDescription)", Array(pLineArray, _
         pTotalValues, pTotalDescription), EA_NORERAISE
    HandleError
End Sub
Private Sub Detail_Format()
    On Error GoTo errHandler
Dim ar() As String
    If iCurRow > UBound(LineArray, 1) Then Exit Sub
    ReDim ar(15)
    ar = Split(LineArray(iCurRow), "|")
    If UBound(ar) > -1 Then fCode = ar(0)
    If UBound(ar) > 0 Then fDescription = ar(1)
    If UBound(ar) > 1 Then fQty = ar(2)
    If UBound(ar) > 2 Then fPrice = ar(3)
    If UBound(ar) > 4 Then fDiscount = ar(5)
    If UBound(ar) > 3 Then fExt = ar(4)
    If UBound(ar) > 4 Then fCostPrice = ar(5)
    iCurRow = iCurRow + 1
    Detail.PrintSection
    Exit Sub
errHandler:
    If ErrMustStop Then Debug.Assert False: Resume
    ErrorIn "arTF_1.Detail_Format", , EA_NORERAISE
    HandleError
End Sub

